[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
quote_type = "double"
space_after_anon_function = true

[*.py]
indent_size = 4

[Makefile]
indent_style = tab

[*.go]
indent_style = tab

[*.{js,ts,jsx,tsx}]
quote_type = "single"